{
  "name": "nestjs-boilerplate",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "postinstall": "is-ci || husky install",
    "typeorm:dev": "env-cmd ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:test": "env-cmd -f .test.env ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm:dev -- --dataSource=src/database/data-source.ts migration:generate",
    "migration:create": "npm run typeorm:dev -- migration:create",
    "migration:run": "npm run typeorm:dev -- --dataSource=src/database/data-source.ts migration:run",
    "migration:revert": "npm run typeorm:dev -- --dataSource=src/database/data-source.ts migration:revert",
    "schema:drop": "npm run typeorm:dev -- --dataSource=src/database/data-source.ts schema:drop",
    "seed:run": "ts-node -r tsconfig-paths/register ./src/database/seeds/run-seed.ts",
    "db:test:rm": "docker compose  -f docker-local-dev.yaml rm test-db -s -v -f",
    "db:test:up": "docker compose -f docker-local-dev.yaml up test-db -d",
    "db:test:wait": "./run/wait-for-it.sh -t 0 -h localhost -p 5435",
    "db:test:migrate": "yarn typeorm:test -- --dataSource=src/database/data-source.ts migration:run",
    "db:test:seed": "env-cmd -f .test.env ts-node -r tsconfig-paths/register ./src/database/seeds/run-seed.ts",
    "db:test:restart": "yarn db:test:rm && yarn db:test:up && yarn db:test:wait && yarn db:test:migrate && yarn db:test:seed",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "yarn db:test:restart  && env-cmd -f .test.env jest --no-cache --config ./test/jest-e2e.json --runInBand"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "1.8.1",
    "@nestjs/bull": "^0.6.3",
    "@nestjs/common": "9.2.1",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "9.2.1",
    "@nestjs/jwt": "9.0.0",
    "@nestjs/microservices": "^10.0.5",
    "@nestjs/passport": "9.0.0",
    "@nestjs/platform-express": "9.2.1",
    "@nestjs/swagger": "6.1.3",
    "@nestjs/throttler": "^4.2.1",
    "@nestjs/typeorm": "9.0.1",
    "@types/amqplib": "^0.10.1",
    "@types/cron": "^2.0.1",
    "@types/lodash": "^4.14.194",
    "@types/uuid": "^9.0.2",
    "amqp-connection-manager": "^4.1.12",
    "amqplib": "^0.10.3",
    "apple-signin-auth": "1.7.4",
    "bcryptjs": "2.4.3",
    "bull": "^4.10.4",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "cron": "^2.3.1",
    "crypto": "^1.0.1",
    "ethereumjs-util": "^7.1.5",
    "ethers": "5.6.9",
    "fb": "2.0.0",
    "google-auth-library": "8.7.0",
    "handlebars": "4.7.7",
    "lodash": "^4.17.21",
    "multer": "1.4.4",
    "multer-s3": "2.10.0",
    "nestjs-i18n": "9.2.3",
    "node-cache": "^5.1.2",
    "nodemailer": "6.8.0",
    "passport": "0.6.0",
    "passport-anonymous": "1.0.1",
    "passport-jwt": "4.0.0",
    "pg": "8.8.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.6.0",
    "source-map-support": "0.5.21",
    "swagger-ui-express": "4.6.0",
    "twitter": "1.7.1",
    "typeorm": "0.3.11",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@nestjs/cli": "9.1.5",
    "@nestjs/schematics": "9.0.3",
    "@nestjs/testing": "9.2.1",
    "@types/bcryptjs": "2.4.2",
    "@types/express": "4.17.14",
    "@types/facebook-js-sdk": "3.3.6",
    "@types/jest": "29.2.3",
    "@types/multer": "1.4.7",
    "@types/node": "16.18.3",
    "@types/passport-anonymous": "1.0.3",
    "@types/passport-jwt": "3.0.7",
    "@types/supertest": "2.0.12",
    "@types/twitter": "1.7.1",
    "@typescript-eslint/eslint-plugin": "5.45.0",
    "@typescript-eslint/parser": "5.45.0",
    "aws-sdk": "2.1243.0",
    "env-cmd": "10.1.0",
    "eslint": "8.28.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "7.0.4",
    "is-ci": "3.0.1",
    "jest": "29.3.1",
    "net": "^1.0.2",
    "pactum": "^3.3.3",
    "prettier": "2.7.1",
    "supertest": "6.3.1",
    "timekeeper": "^2.2.0",
    "ts-jest": "29.0.1",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.1",
    "tslib": "2.4.1",
    "typescript": "4.9.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
